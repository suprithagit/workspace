package forLoops;

public class NestedForLoops {
	public static void main(String[] args) {
		System.out.println("Multiplication table");
		
		for(var i=1;i<=10;i++) {
			System.out.println();
			for(var j=1;j<=10;j++) {
				System.out.print(i+"*"+j+" = "+ i*j+"  ");
			}
		}
	

	for (int i = 1; i <= 10; i++) {
	    for (int j = 1; j <= 10; j++) {
	        System.out.print((i * j) + "\t");
	    }
	    System.out.println();
	}
	
	System.out.println("Right triangle");  
	for (int i = 1; i <= 5; i++) {
	    for (int j = 1; j <= i; j++) {
	        System.out.print("* ");
	    }
	    System.out.println();
	}
	
	}
}
